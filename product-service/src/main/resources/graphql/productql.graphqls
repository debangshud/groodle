type Query {
    products(count: Int):[Product]
    productById(id: ID): Product
}

type Mutation {
    createProduct(name: String!, description: String!, manufacturer: ID!): Product!
}

type Product {
    id: ID!,
    name: String,
    description: String,
    manufacturer: Manufacturer
}

type Manufacturer {
    id: ID!,
    name: String
}