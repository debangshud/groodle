type Query {
    findAll(count: Int):[Product]
    findByID(id: ID): Product
    count: Int
}

type Mutation {
    createProduct(name: String!, description: String!, manufacturer: ID!): Product!
    createManufacturer(name: String!): Manufacturer!
}

type Product {
    id: ID!,
    name: String,
    description: String,
    manufacturer: Manufacturer
}

type Manufacturer {
    id: ID!,
    name: String
}